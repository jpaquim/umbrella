{
	"name": "@thi.ng/math",
	"version": "5.4.11",
	"description": "Assorted common math functions & utilities",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/math#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"contributors": [
		"@nkint (https://github.com/nkint)"
	],
	"license": "Apache-2.0",
	"scripts": {
		"build": "pnpm clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "pnpm doc:stats && tools:readme",
		"doc:stats": "tools:module-stats",
		"pub": "pnpm npm publish --access public",
		"test": "testament test"
	},
	"dependencies": {
		"@thi.ng/api": "^8.8.2"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.35.3",
		"@thi.ng/testament": "^0.3.17",
		"rimraf": "^5.0.1",
		"tools": "workspace:^",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.3"
	},
	"keywords": [
		"animation",
		"bezier",
		"cubic",
		"hermite",
		"interpolation",
		"interval",
		"math",
		"prime",
		"quadratic",
		"smooth",
		"solver",
		"t-norm",
		"trigonometry",
		"typescript"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./abs": {
			"default": "./abs.js"
		},
		"./angle": {
			"default": "./angle.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./crossing": {
			"default": "./crossing.js"
		},
		"./eqdelta": {
			"default": "./eqdelta.js"
		},
		"./extrema": {
			"default": "./extrema.js"
		},
		"./fit": {
			"default": "./fit.js"
		},
		"./int": {
			"default": "./int.js"
		},
		"./interval": {
			"default": "./interval.js"
		},
		"./libc": {
			"default": "./libc.js"
		},
		"./min-error": {
			"default": "./min-error.js"
		},
		"./mix": {
			"default": "./mix.js"
		},
		"./permutations": {
			"default": "./permutations.js"
		},
		"./prec": {
			"default": "./prec.js"
		},
		"./prime": {
			"default": "./prime.js"
		},
		"./ratio": {
			"default": "./ratio.js"
		},
		"./safe-div": {
			"default": "./safe-div.js"
		},
		"./solve": {
			"default": "./solve.js"
		},
		"./step": {
			"default": "./step.js"
		}
	},
	"thi.ng": {
		"year": 2013
	}
}
